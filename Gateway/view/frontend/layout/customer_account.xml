<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="customer_account_navigation">
            <block class="Magento\Framework\View\Element\Html\Link\Current" name="easytransac-cards-list">
                <arguments>
                    <argument name="label" xsi:type="string">EasyTransac Cards</argument>
                    <argument name="path" xsi:type="string">easytransac/cards/index</argument>
                    <argument name="sortOrder" xsi:type="number">50</argument>
                </arguments>
            </block>
        </referenceBlock>
    </body>
</page>
